{
    "name": "dedsecteam17/green-validator",
    "description": "A modern PHP validation library with support for complex validation rules",
    "type": "library",
    "keywords": ["validation", "validator", "form", "data"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Elamin",
            "email": "mohammed@dedsecteam17.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "GreenValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GreenValidator\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
